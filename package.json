  "scripts": {
    "gulp": "NODE_ENV=development gulp",
    "test": "NODE_ENV=test mocha test --recursive",
    "build": "browserify -r --extension=.js --extension=.jsx ./app -d | ./node_modules/.bin/exorcist public/js/build/mayk.js.map > public/js/build/mayk.js",
    "watch": "watchify -r --extension=.js --extension=.jsx ./app -o './node_modules/.bin/exorcist public/js/mayk.js.map > public/js/mayk.js' -d -v",
    "less": "lessc ./public/less/main.less ./public/css/main.css",
    "build-production": "npm run build-production-js && npm run build-production-css",
    "build-production-js": "NODE_ENV=production browserify -r --extension=.js --extension=.jsx ./app | uglifyjs -cm > public/js/mayk.min.js",
    "build-production-css": "npm run less && ./node_modules/.bin/csso public/css/main.css public/css/main.min.css",
    "build-less": "gulp less"
  },
  "browserify-shim": {},
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "keywords": [
    "developer portfolios"
  ]
